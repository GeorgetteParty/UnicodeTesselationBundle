    ,"symfony/config": ">=2.1,<2.3-dev"
    ,"symfony/yaml": ">=2.1,<2.3-dev"
    ,"symfony/http-kernel": ">=2.1,<2.3-dev"
  },
  "autoload": {
    "psr-0": {
